@using CarBook.EntityLayer.Concrete;
@model List<Car>

@{
    ViewData["Title"] = "StatusDetails";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

@foreach (var item in Model)
{
    <div class="container">
        <label>Araba Modeli</label>
        <input type="text" asp-for="@item.Model" class="form-control" />
        <br />
        <label>Model Yılı</label>
        <input type="text" asp-for="@item.Year" class="form-control" />
        <br />
        <label>Vites Türü</label>
        <input type="text" asp-for="@item.Transmission" class="form-control" />
        <br />
    </div>
}